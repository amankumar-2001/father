{
  "name": "Father",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "assets/icon.png"
  },
  "icons": {
    "128": "assets/icon.png"
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["runFastScript.js", "walkScript.js", "skipScript.js"],
      "run_at": "document_idle"
    }
  ]
}
